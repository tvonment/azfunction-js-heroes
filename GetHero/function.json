{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "hero/{id}"
    },
    {
      "name": "heroIn",
      "type": "cosmosDB",
      "connectionStringSetting": "HeroesCore_COSMOSDB",
      "databaseName": "heroes-db",
      "collectionName": "Heroes",
      "id": "{id}",
      "partitionKey": "{id}",
      "direction": "in"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "heroOut",
      "type": "cosmosDB",
      "databaseName": "heroes-db",
      "collectionName": "Heroes",
      "createIfNotExists": true,
      "partitionKey": "{id}",
      "connectionStringSetting": "HeroesCore_COSMOSDB",
      "direction": "out"
    }
  ]
}